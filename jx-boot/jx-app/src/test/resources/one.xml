<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Multiple Suites Example" verbose="1">

    <!-- 单纯使用 testng -->
    <test name="testng">
        <classes>
            <class name="com.laolang.sshschema.HelloWorldSchemaTest"/>
        </classes>
    </test>

    <!-- spring 结合 testng -->
    <test name="spring">
        <classes>
            <class name="com.laolang.sshschema.HelloWorldSchemaSpringTest"/>
        </classes>
    </test>
</suite>